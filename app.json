{
  "name": "OpenSea Discord Bot",
  "description": "A TS script to hit the OpenSea API for collection sales and send messages to Discord",
  "keywords": [
    "ethereum",
    "nft",
    "discord"
  ],
  "repository": "https://github.com/sbauch/opensea-discord-bot",
  "env": {
    "CONTRACT_ADDRESS": {
      "description": "0x387e9625a6fbde9f483b8371ff35c10ad8621980"
    },
    "COLLECTION_SLUG": {
      "description": "The OpenSea slug for your collection"
    },
    "DISCORD_BOT_TOKEN": {
      "description": "ODgwOTc1MzE1OTk5MDE5MDE4.YSmGMw.vGVEAg0gySgyRUTfVbSs3T1qAvE",
      "required": true
    },
    "DISCORD_CHANNEL_ID": {
      "description": "873441666235793438",
      "required": false
    },
    "SECONDS": {
      "description": "1800",
      "required": false
    }
  },
  "image": "heroku/nodejs",
  "addons": ["scheduler"],
  "formation": [],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    }
  ]
}
